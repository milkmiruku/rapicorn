/**
 * @file
 * @brief Aida builtin type definitions.
 */

namespace Aida {
  namespace __system_typedefs__ {
    // special namespace, containing definitions for the builtin TypeMap
    typedef bool	__builtin__bool;
    typedef int32 	__builtin__int32;
    typedef int64 	__builtin__int64;
    typedef float64	__builtin__float64;
    typedef String  	__builtin__String;
    typedef Any    	__builtin__Any;
    typedef Any    	__builtin__AidaLocal;
    typedef Any    	__builtin__AidaRemote;
  } // __system_typedefs__
  sequence DynamicSequence { Any any; };
  record   DynamicRecord   { Any __builtin__deleteme__; };
  enum     DynamicEnum     { __builtin__deleteme__ };
} // Aida
