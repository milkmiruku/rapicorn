<rapicorn-definitions xmlns:Rapicorn_Factory="http://rapicorn.org/xmlns">

  <!-- Alignment -->
  <Rapicorn_Factory:Alignment id="Alignment"/>

  <!-- Ambience (background + lighting) -->
  <Rapicorn_Factory:Ambience id="Ambience"/>

  <!-- Arrangement -->
  <Rapicorn_Factory:Arrangement id="Arrangement" />

  <!-- Arrow -->
  <Rapicorn_Factory:Arrow id="Arrow"/>

  <!-- buttons -->
  <Rapicorn_Factory:ButtonArea id="Button" child-container="child-area" >
    <Argument id="center-child" default="1" />
    <Frame normal-frame="out" impressed-frame="in" >
      <Ambience
	normal-lighting="upper-left" impressed-lighting="lower-right" prelight-lighting="upper-left" insensitive-lighting="none"
	normal-shade="none" impressed-shade="none" prelight-shade="diffuse" insensitive-shade="none">
	<FocusFrame frame-type="none">
	  <Alignment id="child-area" vscale="`not 1*center_child`" hscale="`not 1*center_child`" left-padding="1" right-padding="1" bottom-padding="1" top-padding="1" />
	</FocusFrame>
      </Ambience>
    </Frame>
  </Rapicorn_Factory:ButtonArea>
  <Rapicorn_Factory:ButtonArea id="UnfocusedButton" child-container="child-area" >
    <Argument id="center-child" default="1" />
    <Frame normal-frame="out" impressed-frame="in" >
      <Ambience
	normal-lighting="upper-left" impressed-lighting="lower-right" prelight-lighting="upper-left" insensitive-lighting="none"
	normal-shade="none" impressed-shade="none" prelight-shade="diffuse" insensitive-shade="none">
	<Alignment id="child-area" vscale="`not 1*center_child`" hscale="`not 1*center_child`" left-padding="1" right-padding="1" bottom-padding="1" top-padding="1" />
      </Ambience>
    </Frame>
  </Rapicorn_Factory:ButtonArea>
  <Rapicorn_Factory:Frame id="DefaultButton" child-container="child-area" frame-type="in" >
    <Argument id="on-click" />
    <Alignment left-padding="2" right-padding="2" bottom-padding="2" top-padding="2" >
      <Button id="child-area" on-click="`on_click`"/>
    </Alignment>
  </Rapicorn_Factory:Frame>

  <!-- DotGrid -->
  <Rapicorn_Factory:DotGrid id="DotGrid"/>

  <!-- Drawable -->
  <Rapicorn_Factory:Drawable id="Drawable"/>

  <!-- FillAreaContainer -->
  <Rapicorn_Factory:FillAreaContainer id="FillAreaContainer"/>

  <!-- frames -->
  <Rapicorn_Factory:Frame id="Frame"/>
  <Rapicorn_Factory:FocusFrame id="FocusFrame"/>

  <!-- Image -->
  <Rapicorn_Factory:Image id="Image" />

  <!-- Label -->
  <Rapicorn_Factory:Label id="Label" />
  <Rapicorn_Factory:TextPango id="Rapicorn_Factory_TextBlock" />

  <!-- LayerPainter -->
  <Rapicorn_Factory:LayerPainter id="LayerPainter"/>

  <!-- StatePainter -->
  <Rapicorn_Factory:StatePainter id="StatePainter"/>

  <!-- Window -->
  <Rapicorn_Factory:Window id="Window"/>

  <!-- slider -->
  <Rapicorn_Factory:SliderTrough id="Rapicorn-slider-trough"/>
  <Rapicorn_Factory:SliderSkid id="Rapicorn-slider-skid"/>
  <!-- HSlider -->
  <Rapicorn_Factory:SliderArea id="HSlider">
    <Frame normal-frame="in" vexpand="1" hexpand="1" height="18">
      <HBox vexpand="1">
	<UnfocusedButton on-click="decrement" on-click2="page-decrement" click-type="fast-repeat" vexpand="1" center-child="0" >
	  <Arrow arrow-dir="left" size-policy="width-from-height"/>
	</UnfocusedButton>
	<Rapicorn-slider-trough hexpand="1" vexpand="1">
	  <Rapicorn-slider-skid vspread="1" hexpand="1" vertical-skid="0">
	    <Ambience
	      normal-lighting="upper-left" impressed-lighting="lower-right" prelight-lighting="upper-left" insensitive-lighting="none"
	      normal-shade="none" impressed-shade="none" prelight-shade="diffuse" insensitive-shade="none">
	      <Frame normal-frame="out" vexpand="1">
		<DotGrid n-hdots="6" n-vdots="2" left-padding-dots="2" right-padding-dots="2" top-padding-dots="0" bottom-padding-dots="0" />
	      </Frame>
	    </Ambience>
	  </Rapicorn-slider-skid>
	</Rapicorn-slider-trough>
	<UnfocusedButton on-click="increment" on-click2="page-increment" click-type="fast-repeat" vexpand="1" center-child="0" >
	  <Arrow arrow-dir="right" size-policy="width-from-height"/>
	</UnfocusedButton>
      </HBox>
    </Frame>
  </Rapicorn_Factory:SliderArea>
  <!-- VSlider -->
  <Rapicorn_Factory:SliderArea id="VSlider">
    <Frame normal-frame="in" vexpand="1" hexpand="1" width="18">
      <VBox hexpand="1">
	<UnfocusedButton on-click="decrement" on-click2="page-increment" click-type="fast-repeat" hexpand="1" center-child="0" >
	  <Arrow arrow-dir="up" size-policy="height-from-width"/>
	</UnfocusedButton>
	<Rapicorn-slider-trough vexpand="1" hexpand="1">
	  <Rapicorn-slider-skid hspread="1" vexpand="1" vertical-skid="1">
	    <Ambience
	      normal-lighting="upper-left" impressed-lighting="lower-right" prelight-lighting="upper-left" insensitive-lighting="none"
	      normal-shade="none" impressed-shade="none" prelight-shade="diffuse" insensitive-shade="none">
	      <Frame normal-frame="out" hexpand="1">
		<DotGrid n-hdots="2" n-vdots="6" left-padding-dots="0" right-padding-dots="0" top-padding-dots="2" bottom-padding-dots="2" />
	      </Frame>
	    </Ambience>
	  </Rapicorn-slider-skid>
	</Rapicorn-slider-trough>
	<UnfocusedButton on-click="increment" on-click2="page-decrement" click-type="fast-repeat" hexpand="1" center-child="0" >
	  <Arrow arrow-dir="down" size-policy="height-from-width"/>
	</UnfocusedButton>
      </VBox>
    </Frame>
  </Rapicorn_Factory:SliderArea>

  <!-- ScrollArea -->
  <Rapicorn_Factory:ScrollPort id="RapicornScrollPort"/>
  <Rapicorn_Factory:ScrollArea id="RapicornScrollArea"/>
  <Alignment id="ScrollArea" child-container="scroll-container">
    <RapicornScrollArea>
      <Table>
	<Alignment hexpand="1" vexpand="1" hposition="0" hspan="1" vposition="0" vspan="1" width="0" height="0"
	  left-padding="2" right-padding="3" bottom-padding="2" top-padding="2">
	  <Frame frame-type="in" hscale="0" vscale="0">
	    <RapicornScrollPort id="scroll-container">
	      <!-- <Ambience id="scroll-container" normal-lighting="upper-left"/> -->
	    </RapicornScrollPort>
	  </Frame>
	</Alignment>
	<VSlider vexpand="0" adjustment-source="ancestry-vertical"   vposition="0" hposition="1" />
	<HSlider hexpand="0" adjustment-source="ancestry-horizontal" vposition="1" />
      </Table>
    </RapicornScrollArea>
  </Alignment>

  <!-- Table -->
  <Rapicorn_Factory:Table id="Table" >
  </Rapicorn_Factory:Table>
  <Rapicorn_Factory:HBox id="HBox" />
  <Rapicorn_Factory:VBox id="VBox" />

  <!-- IdlTestWidget -->
  <Rapicorn_Factory:IdlTestWidget id="IdlTestWidget"/>

  <!-- TestBox -->
  <Rapicorn_Factory:TestBox id="TestBox"/>

  <!-- TestContainer -->
  <Rapicorn_Factory:TestContainer id="TestContainer"/>

  <!-- TestWidget -->
  <TestContainer id="TestWidget" paint-allocation="1" width="10" height="10"/>

  <!-- TextEditor -->
  <Rapicorn_Factory:TextEditor id="TextEditor"/>

</rapicorn-definitions>
